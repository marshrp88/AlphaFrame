# AlphaFrame Production Environment Configuration
# Copy this file to .env.prod and fill in your actual values

# Plaid Configuration (Production)
VITE_PLAID_CLIENT_ID=your_plaid_client_id_here
VITE_PLAID_SECRET=your_plaid_production_secret_here
VITE_PLAID_ENV=production

# API Configuration
VITE_API_URL=https://api.alphaframe.com/api
VITE_APP_ENV=production

# Authentication (Production)
VITE_AUTH_DOMAIN=your_auth0_domain_here
VITE_AUTH_CLIENT_ID=your_auth0_client_id_here
VITE_AUTH_AUDIENCE=your_auth0_audience_here

# Webhook Configuration
VITE_WEBHOOK_URL=https://api.alphaframe.com/webhooks
VITE_WEBHOOK_SECRET=your_webhook_secret_here

# Notification Service
VITE_SENDGRID_API_KEY=your_sendgrid_api_key_here
VITE_NOTIFICATION_FROM_EMAIL=noreply@alphaframe.com

# Feature Flags
VITE_ENABLE_BETA_MODE=false
VITE_ENABLE_PLAID_INTEGRATION=true
VITE_ENABLE_WEBHOOKS=true
VITE_ENABLE_NOTIFICATIONS=true

# Logging
VITE_LOG_LEVEL=warn
VITE_ENABLE_DEBUG_MODE=false

# Security
VITE_ENCRYPTION_KEY=your_encryption_key_here
VITE_JWT_SECRET=your_jwt_secret_here 